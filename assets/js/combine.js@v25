]")[0], chb2 = $("[data-productid=" + hdComp2.value + "]")[0], chb3 = $("[data-productid=" + hdComp3.value + "]")[0], chb4 = $("[data-productid=" + hdComp4.value + "]")[0], "1" == e ? (hdImg1.value = "", hdComp1.value = "0", divComp1.className = "", null != chb1 && (chb1.checked = !1)) : "2" == e ? (hdImg2.value = "", hdComp2.value = "0", divComp2.className = "", null != chb2 && (chb2.checked = !1)) : "3" == e ? (hdImg3.value = "", hdComp3.value = "0", divComp3.className = "", null != chb3 && (chb3.checked = !1)) : "4" == e && (hdImg4.value = "", hdComp4.value = "0", divComp4.className = "", null != chb4 && (chb4.checked = !1)), "0" == hdComp1.value && "0" != hdComp2.value && (hdImg1.value = hdImg2.value, hdComp1.value = hdComp2.value, hdImg2.value = "", hdComp2.value = "0"), "0" == hdComp2.value && "0" != hdComp3.value && (hdImg2.value = hdImg3.value, hdComp2.value = hdComp3.value, hdImg3.value = "", hdComp3.value = "0"), "0" == hdComp3.value && "0" != hdComp4.value && (hdImg3.value = hdImg4.value, hdComp3.value = hdComp4.value, hdImg4.value = "", hdComp4.value = "0"), "0" != hdComp1.value ? divComp1.className = "selected" : divComp1.className = "", "0" != hdComp2.value ? divComp2.className = "selected" : divComp2.className = "", "0" != hdComp3.value ? divComp3.className = "selected" : divComp3.className = "", "0" != hdComp4.value ? divComp4.className = "selected" : divComp4.className = "", divComp1.style.backgroundImage = "url('" + hdImg1.value + "')", divComp2.style.backgroundImage = "url('" + hdImg2.value + "')", divComp3.style.backgroundImage = "url('" + hdImg3.value + "')", divComp4.style.backgroundImage = "url('" + hdImg4.value + "')", "0" != hdComp1.value ? $(".productComparison").css("display", "") : $(".productComparison").css("display", "none"), SetCompareCookie() } function SetCompareCookie() { hdComp1 = document.getElementById("hdComp1"), hdComp2 = document.getElementById("hdComp2"), hdComp3 = document.getElementById("hdComp3"), hdComp4 = document.getEl